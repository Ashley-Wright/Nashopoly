  %body
    %header
      #image_holder
        %img{src:'../images/nashopoly_logo_full.png', alt:'Nashopoly logo'}
      #wrapper1
      #wrapper2
        %navigation#nav1
          %ul
            %li
              %a{href:"../index.html"}HOME
            %li
              %a{href:"board.html"}BOARD
            %li
