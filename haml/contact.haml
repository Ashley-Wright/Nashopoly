      #image_holder
        %img{src:'../images/nashopoly_logo_small.png', alt:'Nashopoly logo'}
      #wrapper1
        %navigation#nav1
          %ul
            %li
              %a{href:"../index.html"}HOME
            %li
              %a{href:"board.html"}BOARD
        %navigation#nav2
          %ul
            %li
              %a{href:"cards.html"}CARDS
            %li
              %a{href:"pieces.html"}PIECES
            %li
              %a{href:"faq.html"}FAQS
            %li
              %a{href:"contact.html"}CONTACT
